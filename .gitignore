# .gitignore for .NET 8.0 projects
# Keep build outputs, IDE files, secrets and other generated artifacts out of source control.


.gitconfig

# Build outputs
[Bb]in/
[Oo]bj/
artifacts/
publish/
app.publish/

# Rider / JetBrains
.idea/
*.sln.iml
*.DotSettings.user
_ReSharper*/
*.ReSharper*/

# Visual Studio
.vs/
*.suo
*.user
*.userosscache
*.sln.docstates

# Visual Studio Code
.vscode/
.vscode/*-workspace.json

# NuGet and packages
packages/
*.nupkg
*.snupkg
.nuget/packages/
.project.lock.json
project.fragment.lock.json

# .NET SDK local files and tools
.dotnet/
.tools/
dotnet-tools.json

# Test results and coverage
TestResults/
*.trx
*.coverage
*.coveragexml
coverage/
coverage*.json
coverage*.xml

# Logs and temporary files
*.log
*.tmp
*.temp
*.pid

# Publish and deployment
*.Publish.xml
.publishsettings
.publish/

# User secrets and local settings (do NOT commit secrets)
local.settings.json
secrets.json
secrets.*
appsettings.*.Local
appsettings.*.Secret

# Database files
*.mdf
*.ldf
*.db
*.sqlite
*.sqlite3

# Symbol and build artifacts
*.pdb
*.ilk
*.meta
*.cache

# Package managers & front-end
node_modules/
wwwroot/lib/

# OS generated files
.DS_Store
Thumbs.db

# Misc / legacy Visual Studio files
*.vspscc
*.vssscc
*.ncb
*.sdf

# Optional: Ignore user-specific launch settings generated by IDEs
# Uncomment if you don't want to commit them:
# Properties/launchSettings.json